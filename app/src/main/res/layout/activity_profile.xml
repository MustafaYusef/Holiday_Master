<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Profile" android:background="#fff">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.0">
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="422dp"
                android:layout_height="608dp" android:background="@drawable/backsearch">
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="606dp">
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="96dp"
                        android:layout_height="66dp">
                    <ImageButton
                            android:layout_width="82dp"
                            android:layout_height="78dp" app:srcCompat="?attr/actionModeCloseDrawable"
                            android:id="@+id/btnBack2" android:background="#6F499C"
                            style="@android:style/Widget.DeviceDefault.Light.ImageButton" android:onClick="backsearch"
                            android:layout_weight="1" android:layout_gravity="left"/>
                </LinearLayout>
                <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="140dp" app:srcCompat="@drawable/iconfinder_instagram"
                        android:id="@+id/imageView"/>
                <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="101dp">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="133dp"
                                android:layout_height="65dp">
                            <ImageButton
                                    android:layout_width="102dp"
                                    android:layout_height="53dp"
                                    android:id="@+id/imageButton41" android:layout_weight="1"
                                    android:background="@drawable/btnright" tools:src="@drawable/iconfinder_instagram"
                                    android:scaleType="centerCrop"/>
                        </LinearLayout>
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" android:gravity="center">
                            <TextView
                                    android:text="TextView"
                                    android:layout_width="match_parent"
                                    android:layout_height="78dp" android:id="@+id/username"
                                    android:textSize="30sp" android:textStyle="bold" android:textAlignment="textStart"
                                    android:textColor="@color/cardview_dark_background"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="89dp">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="75dp">
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="138dp"
                                android:layout_height="64dp">
                            <ImageButton
                                    android:layout_width="100dp"
                                    android:layout_height="39dp"
                                    android:id="@+id/imageButton42" android:layout_weight="1"
                                    android:background="@drawable/btnright" tools:src="@drawable/iconfinder_instagram"
                                    android:scaleType="centerCrop"/>
                        </LinearLayout>
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" android:gravity="center">
                            <TextView
                                    android:text="TextView"
                                    android:layout_width="match_parent"
                                    android:layout_height="74dp" android:id="@+id/Email"
                                    android:textSize="30sp" android:textStyle="bold" android:textAlignment="textStart"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="83dp">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="78dp">
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="138dp"
                                android:layout_height="60dp">
                            <ImageButton
                                    android:layout_width="100dp"
                                    android:layout_height="match_parent"
                                    android:id="@+id/imageButton44" android:layout_weight="1"
                                    android:background="@drawable/btnright" tools:src="@drawable/iconfinder_instagram"
                                    android:scaleType="centerCrop"/>
                        </LinearLayout>
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" android:gravity="center">
                            <TextView
                                    android:text="TextView"
                                    android:layout_width="match_parent"
                                    android:layout_height="74dp" android:id="@+id/Phone"
                                    android:textSize="30sp" android:textStyle="bold" android:textAlignment="textStart"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="79dp">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="138dp"
                                android:layout_height="63dp">
                            <ImageButton
                                    android:layout_width="102dp"
                                    android:layout_height="match_parent"
                                    android:id="@+id/imageButton4" android:layout_weight="1"
                                    android:background="@drawable/btnright" tools:src="@drawable/iconfinder_instagram"
                                    android:scaleType="centerCrop"/>
                        </LinearLayout>
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" android:gravity="center">
                            <TextView
                                    android:text="TextView"
                                    android:layout_width="match_parent"
                                    android:layout_height="74dp" android:id="@+id/Money"
                                    android:textSize="30sp" android:textStyle="bold" android:textAlignment="textStart"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:gravity="center|top">
            <Button
                    android:text="Logout"
                    android:layout_width="274dp"
                    android:layout_height="65dp" android:id="@+id/button11" android:background="@drawable/btnun"
                    android:layout_marginTop="5dp" android:textSize="24sp" android:textStyle="bold"
                    android:onClick="Logout"/>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>