<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".login.profile.Profile">
<androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".login.profile.Profile" android:background="#fff" android:paddingBottom="10dp">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.0">
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:background="@drawable/backsearch">
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="547dp">
                <!--<LinearLayout-->
                <!--android:orientation="horizontal"-->
                <!--android:layout_width="40dp"-->
                <!--android:layout_height="50dp">-->
                <!--<ImageButton-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="23dp" app:srcCompat="@drawable/left_arrow23"-->
                <!--android:id="@+id/btnBack2" android:background="#6F499C"-->
                <!--style="@android:style/Widget.DeviceDefault.Light.ImageButton" android:onClick="backsearch"-->
                <!--android:layout_weight="1" android:scaleType="centerInside" android:layout_marginTop="10dp"/>-->
                <!--</LinearLayout>-->
                <ImageView
                        android:id="@+id/imageView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:layout_marginBottom="25dp"
                        app:srcCompat="@drawable/user5" />
                <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:layout_marginBottom="10dp">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:gravity="center">
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="81dp"
                                android:layout_height="45dp" android:background="@drawable/btnright"
                                android:gravity="center">
                            <View
                                    android:layout_width="42dp"
                                    android:layout_height="39dp" android:id="@+id/view"
                                    android:background="@drawable/user5" android:layout_gravity="center"/>
                        </LinearLayout>
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" android:gravity="center">
                            <TextView
                                    android:text="Name"
                                    android:layout_width="match_parent"
                                    android:layout_height="52dp" android:id="@+id/username"
                                    android:textSize="22sp" android:textStyle="normal"
                                   android:gravity="center_vertical"
                                    android:textColor="#fff" android:paddingLeft="20dp"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:gravity="center">
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="84dp"
                                android:layout_height="45dp" android:background="@drawable/btnright"
                                android:gravity="center">
                            <View
                                    android:layout_width="42dp"
                                    android:layout_height="39dp" android:id="@+id/view2"
                                    android:background="@drawable/email11"/>
                        </LinearLayout>
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:gravity="center">
                            <TextView
                                    android:text="Email"
                                    android:layout_width="match_parent"
                                    android:layout_height="52dp" android:id="@+id/Email"
                                    android:textSize="22sp"
                                    android:gravity="center_vertical"
                                    android:textColor="#fff" android:paddingLeft="20dp"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                        android:layout_marginBottom="10dp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" android:gravity="center">
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="81dp"
                                android:layout_height="45dp" android:background="@drawable/btnright"
                                android:gravity="center">
                            <View
                                    android:layout_width="42dp"
                                    android:layout_height="39dp" android:id="@+id/view4"
                                    android:background="@drawable/phone_contact"/>
                        </LinearLayout>
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:gravity="center">
                            <TextView
                                    android:text="Phone Number"
                                    android:layout_width="match_parent"
                                    android:layout_height="52dp" android:id="@+id/Phone"
                                    android:textSize="22sp"
                                    android:gravity="center_vertical"
                                    android:textColor="#fff" android:paddingLeft="20dp"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" android:gravity="center">
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="83dp"
                                android:layout_height="45dp" android:background="@drawable/btnright"
                                android:gravity="center">
                            <View
                                    android:layout_width="42dp"
                                    android:layout_height="35dp" android:id="@+id/view5"
                                    android:background="@drawable/dollar_symbol" android:layout_gravity="center"/>
                        </LinearLayout>
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" android:gravity="center">
                            <TextView
                                    android:text="Mony"
                                    android:layout_width="match_parent"
                                    android:layout_height="53dp" android:id="@+id/Money"
                                    android:textSize="22sp"
                                    android:gravity="center_vertical"
                                    android:textColor="#fff" android:paddingLeft="20dp"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_marginBottom="20dp"
                android:layout_width="match_parent"
                android:layout_height="63dp" android:gravity="right|top" android:layout_marginTop="-25dp">

            <Button
                    android:layout_marginBottom="20dp"
                    android:text="Logout"
                    android:layout_width="200dp"
                    android:layout_height="60dp"
                    android:id="@+id/LogOUT" android:background="@drawable/btn"
                    android:textSize="24sp"
                    android:textColor="#fff" />
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>