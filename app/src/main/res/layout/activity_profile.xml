<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Profile" android:background="#fff">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="648dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.0">
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="422dp"
                android:layout_height="573dp" android:background="@drawable/backsearch">
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="567dp">
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="96dp"
                        android:layout_height="66dp">
                    <ImageButton
                            android:layout_width="82dp"
                            android:layout_height="match_parent" app:srcCompat="?attr/actionModeCloseDrawable"
                            android:id="@+id/btnBack2" android:background="#6F499C"
                            style="@android:style/Widget.DeviceDefault.Light.ImageButton" android:onClick="backsearch"
                            android:layout_weight="1" android:layout_gravity="left"/>
                </LinearLayout>
                <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="118dp" app:srcCompat="@drawable/user_profile"
                        android:id="@+id/imageView"/>
                <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="101dp">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="84dp">
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="89dp"
                                android:layout_height="61dp" android:background="@drawable/btnright">
                            <View
                                    android:layout_width="84dp"
                                    android:layout_height="54dp" android:id="@+id/view"
                                    android:background="@drawable/user"/>
                        </LinearLayout>
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" android:gravity="center">
                            <TextView
                                    android:text="TextView"
                                    android:layout_width="match_parent"
                                    android:layout_height="65dp" android:id="@+id/username"
                                    android:textSize="30sp" android:textStyle="normal"
                                    android:textAlignment="gravity"
                                    android:textColor="#fff" android:paddingLeft="20dp"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="89dp">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="75dp">
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="84dp"
                                android:layout_height="61dp" android:background="@drawable/btnright">
                            <View
                                    android:layout_width="73dp"
                                    android:layout_height="51dp" android:id="@+id/view2"
                                    android:background="@drawable/opened_email"/>
                        </LinearLayout>
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" android:gravity="center">
                            <TextView
                                    android:text="TextView"
                                    android:layout_width="match_parent"
                                    android:layout_height="74dp" android:id="@+id/Email"
                                    android:textSize="30sp" android:textAlignment="gravity"
                                    android:textColor="#fff" android:paddingLeft="20dp"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="83dp">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="78dp">
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="81dp"
                                android:layout_height="52dp" android:background="@drawable/btnright">
                            <View
                                    android:layout_width="68dp"
                                    android:layout_height="47dp" android:id="@+id/view4"
                                    android:background="@drawable/phone_numbers"/>
                        </LinearLayout>
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" android:gravity="center">
                            <TextView
                                    android:text="TextView"
                                    android:layout_width="match_parent"
                                    android:layout_height="74dp" android:id="@+id/Phone"
                                    android:textSize="30sp" android:textAlignment="gravity"
                                    android:textColor="#fff" android:paddingLeft="20dp"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="79dp">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="83dp"
                                android:layout_height="53dp" android:background="@drawable/btnright">
                            <View
                                    android:layout_width="84dp"
                                    android:layout_height="49dp" android:id="@+id/view5"
                                    android:background="@drawable/coin"/>
                        </LinearLayout>
                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" android:gravity="center">
                            <TextView
                                    android:text="TextView"
                                    android:layout_width="match_parent"
                                    android:layout_height="74dp" android:id="@+id/Money"
                                    android:textSize="30sp" android:textAlignment="gravity"
                                    android:textColor="#fff" android:paddingLeft="20dp"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_marginBottom="20dp"
                android:layout_width="match_parent"
                android:layout_height="68dp" android:gravity="center|top">

            <Button
                    android:text="Logout"
                    android:layout_width="212dp"
                    android:layout_height="65dp" android:id="@+id/button11" android:background="@drawable/btnun"
                    android:layout_marginTop="5dp" android:textSize="24sp" android:textStyle="bold"
                    android:onClick="Logout"/>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>